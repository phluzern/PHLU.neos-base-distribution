{
  "rules": [{
    "$prefix": {
      "$uid": {
        ".write": "$uid === auth.uid",
        ".read": "$uid === auth.uid"
      }
    },
    "sites": {
      "corporate": {
        ".read": true
      }
    },
    "logstore": {
      "corporate": {
        "$workspace": {
          "$dimension": {
            "$uid": {
              ".write": true,
              ".read": true
            }
          }
        }
      }
    },
    "documentation": {
      ".read": true
    },
    "branches": {
      ".read": true
    },
    "lastsync": {
      ".read": true
    }
  }
}